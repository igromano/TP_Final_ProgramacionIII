                            <%if (Session["usuario"] == null)
                                { %>

                            <a class="nav-link" href="Login.aspx" role="button" aria-expanded="false">Iniciar Session</a>
                            <a class="nav-link" href="Registrar.aspx" role="button" aria-expanded="false">Registrate</a>

                            <% } %>
                            <% else
                                { %>

                            <%-- <a class="nav-link dropdown-toggle" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false"><% Session['usuario']['nombre']%></a> --%>

                            <a class="nav-link" href="Login.aspx" role="button" aria-expanded="false">
                                <i class="fas fa-solid fa-headphones"></i> Ayuda</a>
                            <a class="nav-link" href="MisServicios.aspx" role="button" aria-expanded="false">
